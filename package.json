{
  "name": "pocketwatch",
  "version": "1.1.0",
  "author": "jakethedev",
  "license": "ISC",
  "description": "A simple stopwatch with laps and the ability to copy times (or get them from the console) - with hardwired global shortcuts, allowing for Streamdeck integration",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "forge": "electron-forge start",
    "interactive": "electron . -i",
    "test": "echo 'All tests pass, nice work! Absolutely nothing is broken!'",
    "shipit": "npm run make",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "debug": "^4.3.4",
    "electron": "^29.3.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
